{
  "name": "github_jobs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "browserSync": "browser-sync start --index public/index.html --browser brave --server --files=src",
    "browserSyncDist": "browser-sync start --index dist/index.html --browser brave --server --files=src",
    "build-html": "cp public/index.html dist/",
    "build-assets": "cp -r ./src/assets/ dist/src",
    "build-rollup": "./node_modules/.bin/rollup src/components/app/src/GithubJobsApp.js --file dist/src/components/app/GithubJobsApp.js --format umd --name GithubJobsApp -m",
    "build-terser": "./node_modules/.bin/terser ./dist/src/components/app/GithubJobsApp.js -o ./dist/src/components/app/GithubJobsApp.js -m",
    "build-app": "npm run build-html && npm run build-assets && npm run build-rollup && npm run build-terser"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "navigo": "^8.10.1"
  },
  "devDependencies": {
    "rollup": "^2.39.0",
    "terser": "^5.6.0"
  }
}
